<templateSet group="CSS">
  <template name="ir" value="text-indent: 100%;&#10;white-space: nowrap;&#10;overflow: hidden;&#10;display:block;" description="Image Replacement" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="&amp;hf" value="&amp;:hover, &amp;:focus {&#10;$END$&#10;}" description="Hover &amp; Focus" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="cf" value="$NAME$:before, $NAME$:after { content: &quot;&quot;; display: table; line-height: 0; }&#10;$NAME$:after { clear: both; }&#10;$NAME$ { *zoom: 1; }" description="Clearfix" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue=".clearfix" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
      <option name="HTML" value="true" />
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="@mmax" value="@media only screen and (max-width: $PX$px) {&#10;    $END$&#10;}&#10;" description="" toReformat="false" toShortenFQNames="true">
    <variable name="PX" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CUCUMBER_FEATURE_FILE" value="false" />
      <option name="CoffeeScript" value="false" />
      <option name="ECMAScript6" value="false" />
      <option name="HAML" value="false" />
      <option name="HTML" value="false" />
      <option name="JAVA_SCRIPT" value="false" />
      <option name="JSON" value="false" />
      <option name="OTHER" value="true" />
      <option name="PHP" value="false" />
      <option name="REQUEST" value="false" />
      <option name="SQL" value="false" />
      <option name="Twig" value="false" />
      <option name="TypeScript" value="false" />
      <option name="Vue" value="false" />
      <option name="XML" value="false" />
    </context>
  </template>
  <template name="@mmin" value="@media only screen and (min-width: $PX$px) {&#10;    $END$&#10;}&#10;" description="" toReformat="false" toShortenFQNames="true">
    <variable name="PX" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CUCUMBER_FEATURE_FILE" value="false" />
      <option name="CoffeeScript" value="false" />
      <option name="ECMAScript6" value="false" />
      <option name="HAML" value="false" />
      <option name="HTML" value="false" />
      <option name="JAVA_SCRIPT" value="false" />
      <option name="JSON" value="false" />
      <option name="OTHER" value="true" />
      <option name="PHP" value="false" />
      <option name="REQUEST" value="false" />
      <option name="SQL" value="false" />
      <option name="Twig" value="false" />
      <option name="TypeScript" value="false" />
      <option name="Vue" value="false" />
      <option name="XML" value="false" />
    </context>
  </template>
</templateSet>